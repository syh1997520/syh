package com.qlgshopping.entity;
/**
 * 评价表
 * @author Administrator
 *
 */
public class Evalute {
private int evaId;
private int odifId;
private String evaMsg;
private int evaScore;
private int userId;
private int proId;
private String evaDate;
private int likeCount;
private int evaReport;
private UserInfo userInfo;//发出评论的用户的实体类
private Product product;//用户评论的商品的实体类
public int getEvaId() {
	return evaId;
}
public void setEvaId(int evaId) {
	this.evaId = evaId;
}
public int getOdifId() {
	return odifId;
}
public void setOdifId(int odifId) {
	this.odifId = odifId;
}
public String getEvaMsg() {
	return evaMsg;
}
public void setEvaMsg(String evaMsg) {
	this.evaMsg = evaMsg;
}
public int getEvaScore() {
	return evaScore;
}
public void setEvaScore(int evaScore) {
	this.evaScore = evaScore;
}
public int getUserId() {
	return userId;
}
public void setUserId(int userId) {
	this.userId = userId;
}
public int getProId() {
	return proId;
}
public void setProId(int proId) {
	this.proId = proId;
}

public String getEvaDate() {
	return evaDate;
}
public void setEvaDate(String evaDate) {
	this.evaDate = evaDate;
}
public int getLikeCount() {
	return likeCount;
}
public void setLikeCount(int likeCount) {
	this.likeCount = likeCount;
}
public int getEvaReport() {
	return evaReport;
}
public void setEvaReport(int evaReport) {
	this.evaReport = evaReport;
}
public UserInfo getUserInfo() {
	return userInfo;
}
public void setUserInfo(UserInfo userInfo) {
	this.userInfo = userInfo;
}
public Product getProduct() {
	return product;
}
public void setProduct(Product product) {
	this.product = product;
}
@Override
public String toString() {
	return "Evalute [evaId=" + evaId + ", odifId=" + odifId + ", evaMsg="
			+ evaMsg + ", evaScore=" + evaScore + ", userId=" + userId
			+ ", proId=" + proId + ", userInfo=" + userInfo + ", product="
			+ product + "]";
}

}
