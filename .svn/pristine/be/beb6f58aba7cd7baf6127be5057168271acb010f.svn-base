package com.qlgshopping.entity;
/**
 * 购物车详情
 * @author Administrator
 *
 */
public class ShoppingCartInfo {
	
	private int shpifId;
	private int shpId;
	private int proId;
	private int stoId;
	private int proNumber;
	private double shpifCost;
	private Store store;
	private ShoppingCart shp;// 购物车的外键实体类
	private Product pro; 
	
	public int getProNumber() {
		return proNumber;
	}
	public void setProNumber(int proNumber) {
		this.proNumber = proNumber;
	}
	public double getShpifCost() {
		return shpifCost;
	}
	public void setShpifCost(double shpifCost) {
		this.shpifCost = shpifCost;
	}
	public int getStoId() {
		return stoId;
	}
	public void setStoId(int stoId) {
		this.stoId = stoId;
	}
	public Store getStore() {
		return store;
	}
	public void setStore(Store store) {
		this.store = store;
	}
	public Product getPro() {
		return pro;
	}
	public void setPro(Product pro) {
		this.pro = pro;
	}
	public int getShpifId() {
		return shpifId;
	}
	public void setShpifId(int shpifId) {
		this.shpifId = shpifId;
	}
	public int getShpId() {
		return shpId;
	}
	public void setShpId(int shpId) {
		this.shpId = shpId;
	}
	public int getProId() {
		return proId;
	}
	public void setProId(int proId) {
		this.proId = proId;
	}
	
	public ShoppingCart getShp() {
		return shp;
	}
	public void setShp(ShoppingCart shp) {
		this.shp = shp;
	}
	@Override
	public String toString() {
		return "ShoppingCartInfo [shpifId=" + shpifId + ", shpId=" + shpId
				+ ", proId=" + proId + ", stoId=" + stoId + ", proNumber="
				+ proNumber + ", shpifCost=" + shpifCost + ", store=" + store
				+ ", shp=" + shp + ", pro=" + pro + "]";
	}
	
	
	
	

}
